{
    "version": 2,
    "builds": [
        {
            "src": "frontend/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "PORT": "8000"
    }
}
